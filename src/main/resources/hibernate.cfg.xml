<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.DerbyDialect</property>
    <property name="hibernate.connection.driver_class">org.apache.derby.jdbc.ClientDriver</property>
    <property name="hibernate.connection.url">jdbc:derby://localhost:1527/snomDB</property>
    <property name="hibernate.connection.username">snom</property>
    <property name="hibernate.connection.password">snomsnom</property>
    <mapping resource="com/r7studio/ioserver/Snomclient.hbm.xml"/>
    <mapping resource="com/r7studio/ioserver/Channel.hbm.xml"/>
    <mapping resource="com/r7studio/ioserver/Ioboard.hbm.xml"/>
    <mapping resource="com/r7studio/ioserver/Modclient.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
